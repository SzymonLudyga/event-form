{
    "parser": "babel-eslint",
    "extends": ["airbnb"],
    "env": {
      "browser": true,
      "node": true
    },
    "rules": {
      "indent": ["error", 4],
      "react/jsx-filename-extension": 0,
      "react/jsx-indent": 0,
      "react/jsx-indent-props": 0,
      "react/no-access-state-in-setstate": 0,
      "react/forbid-prop-types": 0
    }
}